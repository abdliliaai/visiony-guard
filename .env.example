# Vision-Y Environment Configuration
# Copy this file to .env and update the values

# Supabase Configuration
VY_SUPABASE_URL=https://your-project.supabase.co
VY_SUPABASE_ANON_KEY=your-anon-key
VY_SUPABASE_SERVICE_ROLE=your-service-role-key

# ML Service Configuration
VY_ML_URL=http://localhost:8001
VY_YOLO_WEIGHTS=./weights/yolov8n.pt
VY_CONFIDENCE_THRESHOLD=0.5
VY_IOU_THRESHOLD=0.45
VY_ALPR_REGION=us

# Streams Worker Configuration
VY_STREAMS_PORT=8002
VY_HLS_SEGMENT_SECONDS=2
VY_EVENT_PREPOST_SECONDS=5
VY_DETECTION_INTERVAL_MS=5000
VY_OUTPUT_DIR=./output
VY_TEMP_DIR=./temp

# WebSocket Configuration
VY_WS_URL=ws://localhost:8003

# Storage Configuration
VY_STORAGE_RETENTION_DAYS=30
VY_MAX_STORAGE_GB=100

# Email Configuration (Optional)
VY_SMTP_HOST=smtp.gmail.com
VY_SMTP_PORT=587
VY_SMTP_USER=your-email@gmail.com
VY_SMTP_PASS=your-app-password

# Development Settings
DEBUG=false
NODE_ENV=production

# Security
VY_JWT_SECRET=your-jwt-secret-key
VY_API_RATE_LIMIT=100

# Performance Tuning
VY_MAX_CONCURRENT_STREAMS=10
VY_FFMPEG_THREADS=4
VY_ML_BATCH_SIZE=4

# Monitoring
VY_METRICS_ENABLED=true
VY_LOG_LEVEL=info